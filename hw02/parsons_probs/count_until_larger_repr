def count_until_larger(num): #0given
!BLANK = 0  #1given #blanki
!BLANK = num % 10  #1given #blankcheck
while !BLANK:  #1given #blanknum>0
!BLANK //= !BLANK  #2given #blanknum #blank10
!BLANK += !BLANK  #2given #blanki #blank1
if num % 10 > !BLANK:  #2given #blankcheck
return !BLANK  #3given #blanki
return !BLANK  #1given #blank-1
print('DEBUG:', !BLANK)
print('DEBUG:', !BLANK)
